Pragma solidity ^0.4.18

Contract Splitter {
	mapping (address => uint) balances;
address payable public balance
address payable public balance Alice
address Public Carol 
address Public Bob
function Transfer(msg.sender, uint amount) public {
  require (msg.sender == Alice)
  balances[msg.sender] -= amount;
  balances[Carol] += amount/2;
  balances[bob] += amount/2
		emit Transfer(msg.sender, amount);
		return true
	}
	}
